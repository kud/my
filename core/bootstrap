#! /usr/bin/env zsh
if [ ! -d "$HOME/my/" ]; then
  git clone https://github.com/kud/my.git ~/my &&
fi

export MY=$HOME/my
source $MY/core/intro

echo_space

$MY/core/dotfiles
$MY/core/rocknroll
$MY/core/symlink
$MY/core/fonts
$MY/core/osx

open https://github.com/kud/my/blob/master/doc/postinstall.md

exec $SHELL
